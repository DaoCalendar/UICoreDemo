<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <FrameLayout
        android:id="@+id/tbs_wrap_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <com.angcyo.widget.DslButton
        android:id="@+id/test1"
        style="@style/DefaultButtonMinStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/lib_xhdpi"
        android:text="测试1"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.angcyo.widget.DslButton
        android:id="@+id/test2"
        style="@style/DefaultButtonMinStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/lib_xhdpi"
        android:text="测试2"
        app:layout_constraintLeft_toLeftOf="@id/test1"
        app:layout_constraintTop_toBottomOf="@id/test1" />

</androidx.constraintlayout.widget.ConstraintLayout>